{
  "name": "website",
  "description": "Website",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -fr __sapper__",
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "preexport": "run-s clean",
    "export": "NODE_ENV=production sapper export --legacy --entry '/ /404 /cloud-native-web-development /markua-support'",
    "export:move404": "mv __sapper__/export/404/index.html __sapper__/export/404.html && rm -r __sapper__/export/404",
    "postexport": "run-s export:move404 && node generate-sitemap.js",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "images": "run-s images:resize images:convert",
    "images:resize": "run-p images:resize:* images:resize:**",
    "images:resize:cover:preview": "find ./static/blog-posts/ -type f -iname \"cover.jpg\" -exec bash -c '[ ! -f \"$(dirname \"$0\")/$(basename \"$0\" .jpg)-preview.jpg\" ] && convert \"$0\" -quiet -resize 'x323' \"$(dirname \"$0\")/$(basename \"$0\" .jpg)-preview.jpg\"' {} \\;",
    "images:resize:cover:previewlead": "find ./static/blog-posts/ -type f -iname \"cover.jpg\" -exec bash -c '[ ! -f \"$(dirname \"$0\")/$(basename \"$0\" .jpg)-preview-lead.jpg\" ] && convert \"$0\" -quiet -resize 'x473' \"$(dirname \"$0\")/$(basename \"$0\" .jpg)-preview-lead.jpg\"' {} \\;",
    "images:resize:post": "find ./static/blog-posts/ -type f -iname \"*.jpg\" -and -not -iname \"cover*.jpg\" -exec bash -c 'convert \"$0\" -quiet -resize 'x523' \"$0\"' {} \\;",
    "images:convert": "run-p images:convert:*",
    "images:convert:jpgtowebp": "find ./static/blog-posts/ -type f -iname \"*.jpg\" -exec bash -c '[ ! -f \"$(dirname \"$0\")/$(basename \"$0\" .jpg).webp\" ] && convert \"$0\" -quiet -quality 90 \"$(dirname \"$0\")/$(basename \"$0\" .jpg).webp\"' {} \\;",
    "images:convert:jpgtojp2": "find ./static/blog-posts/ -type f -iname \"*.jpg\" -exec bash -c '[ ! -f \"$(dirname \"$0\")/$(basename \"$0\" .jpg).jp2\" ] && convert \"$0\" -quiet -quality 0 \"$(dirname \"$0\")/$(basename \"$0\" .jpg).jp2\"' {} \\;"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@tailwindcss/typography": "^0.1.1",
    "cypress": "^4.8.0",
    "front-matter": "^4.0.2",
    "mdsvex": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "reading-time": "^1.2.0",
    "rehype-picture": "^3.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-dynamic-import-variables": "^1.1.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.9.7",
    "tailwindcss": "^1.4.6"
  }
}
